{
  "name": "scutage",
  "version": "0.0.4",
  "description": "Simple, opinionated Static Site Generator from HTML",
  "author": "Andrew Crites <acjrites@gmail.com> (https://github.com/ajcrites/scutage)",
  "license": "MIT",
  "main": "lib/scutage",
  "bin": "lib/index.js",
  "types": "lib",
  "scripts": {
    "prepublishOnly": "rm -rf lib && tsc && chmod +x lib/index.js",
    "precommit": "lint-staged",
    "test": "tsc --project tsconfig.test.json && tslint --project tsconfig.test.json && jest --coverage"
  },
  "lint-staged": {
    "**/*.ts": [
      "prettier --write --parser typescript --single-quote --trailing-comma all",
      "tslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/yargs": "^11.1.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.2",
    "ts-config-mobiquity-server": "^1.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "repository": "github:ajcrites/scutage",
  "dependencies": {
    "clean-css": "^4.2.1",
    "globby": "^8.0.1",
    "html-minifier": "^3.5.19",
    "jsdom": "^11.12.0",
    "mkdirp-promise": "^5.0.1",
    "mz": "^2.7.0",
    "rmfr": "^2.0.0",
    "uglify-es": "^3.3.9",
    "yargs": "^12.0.1"
  }
}
